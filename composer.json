{
    "name": "codebyray/laravel-review-rateable",
   "description": "Review & Rating system for Laravel 7",
    "keywords": ["rating", "Ratable", "laravel", "Review-Rateable", "reviewable"],
    "license": "MIT",
    "authors": [
        {
            "name": "CodebyRay",
            "email": "dev@codebyray.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php" : "^7.2",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|6.*|7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3|^7.0|^8.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Codebyray\\ReviewRateable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codebyray\\ReviewRateable\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codebyray\\ReviewRateable\\ReviewRateableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:windows": "vendor\\bin\\phpunit"
    }
}